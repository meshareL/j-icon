{
  "name": "@tomoeed/j-icon",
  "version": "4.0.5",
  "description": "A SVG icon component for Vue",
  "author": "Lu Mengyao",
  "type": "module",
  "homepage": "https://github.com/meshareL/j-icon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meshareL/j-icon.git"
  },
  "bugs": {
    "url": "https://github.com/meshareL/j-icon/issues"
  },
  "main": "dist/component/index.umd.min.js",
  "module": "dist/component/index.esm.min.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "dist/component/index.esm.min.js",
      "browser": "dist/component/index.umd.min.js",
      "types": "index.d.ts"
    }
  },
  "files": [
    "bin",
    "dist",
    "index.d.ts"
  ],
  "keywords": [
    "vue",
    "icon",
    "svg"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "j-icon": "bin/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "camelcase": "^8.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "core-js-pure": "^3.37.1",
    "happy-dom": "^14.12.3",
    "prettier": "^3.3.2",
    "svgo": "^3.3.2",
    "type-fest": "^4.20.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^8.57.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@types/js-beautify": "^1.14.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.19.39",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "babel-plugin-polyfill-corejs3": "^0.10.4",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",
    "jsdom": "^24.1.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "vitest": "^1.5.0",
    "vue": "^3.2.47"
  }
}
